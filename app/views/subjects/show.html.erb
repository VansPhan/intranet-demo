<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="container-fluid">
	<h2 class="page-header">Show Subject</h2>

	<table class="table table-hover" summary="Subject detail view">
		<tr>
			<th>Name</th>
			<td><%= @subject.name %></td>
		</tr>
		<tr>
			<th>Position</th>
			<td><%= @subject.position %></td>
		</tr>
		<tr>
			<th>Created</th>
			<td><%= @subject.created_at %></td>
		</tr>
		<tr>
			<th>Updated</th>
			<td><%= @subject.updated_at %></td>
		</tr>
	</table>
</div>